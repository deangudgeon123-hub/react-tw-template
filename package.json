    "ethers": "^6.14.0",
    "i18next": "^24.2.2",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.487.0",
    "motion": "^12.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^15.5.1",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^7.1.5",
